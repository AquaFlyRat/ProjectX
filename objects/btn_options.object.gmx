            <string>///draw
if(active){
    if guifade&lt;1{guifade = lerp(guifade,1,.1)}
    ///vars
    var vw = view_wview*obj_view.view_zoom;
    var vh = view_hview*obj_view.view_zoom;
    var sc = 1;
    var p1 = "";
    if active ==1{p1 = "fullscreen/windowed";}
    if active ==2{p1 = "display size";}
    if active ==3{p1 = "volume:"+string(floor(global.maxvol*100));}
    ///background
    draw_set_color(c_black);
    draw_set_alpha(.5*guifade);
    draw_rectangle(-8,-8,(vw)+8,(vh)+8,false);
    ///prompt
    var n1 = "use arrows to change"
    var n2 = "&lt;backspace/enter &gt;"
    var n3 = "x esc x"
    var n4 = ""
    text_draw(vw*.5-text_length(p1,spr_font_a,sc)*.5,vh*.5-45,p1,spr_font_a,c_white,sc,1*guifade,false);
    text_draw(vw*.5-text_length(n1,spr_font_a,sc)*.5,vh*.5-15,n1,spr_font_a,c_white,sc,1*guifade,false);
    text_draw(vw*.5-text_length(n2,spr_font_a,sc)*.5,vh*.5,n2,spr_font_a,c_white,sc,1*guifade,false);
    text_draw(vw*.5-text_length(n3,spr_font_a,sc)*.5,vh*.5+15,n3,spr_font_a,c_white,sc,1*guifade,false);
    text_draw(vw*.5-text_length(n4,spr_font_a,sc)*.5,vh*.5+30,n4,spr_font_a,c_white,sc,1*guifade,false);
}else{
    if guifade&gt;0{guifade = lerp(guifade,0,.1)}
}
draw_set_alpha(1);
