        <arguments>
          <argument>
            <kind>1</kind>
            <string>///record
if (active==1){
    //fullscreen
    if keyboard_check_pressed(vk_right) || keyboard_check_pressed(vk_left){
        window_set_fullscreen(!window_get_fullscreen())
    }
    if keyboard_check_pressed(vk_enter) &amp;&amp; cooldown&lt;=0{
        if window_get_fullscreen(){
            active=3;
            cooldown = 5;
        }else{
            active=2;
            cooldown = 5;
        }
    }
}
if (active==2){
    //display
    with (obj_display){
        if keyboard_check_pressed(vk_left){
            if window_scale&gt;1{
                window_scale--;
                alarm[0] = 1;
            }
            //update layers
            surface_resize(application_surface,ideal_width,ideal_height);
            display_set_gui_size(ideal_width,ideal_height);
            window_set_size(ideal_width*window_scale,ideal_height*window_scale);
        }
        if keyboard_check_pressed(vk_right){
            if ((window_scale+1)*ideal_width&lt;display_get_width()){
                window_scale++;
                alarm[0] = 1;
            }
            //update layers
            surface_resize(application_surface,ideal_width,ideal_height);
            display_set_gui_size(ideal_width,ideal_height);
            window_set_size(ideal_width*window_scale,ideal_height*window_scale);
        }
    }
    if keyboard_check_pressed(vk_enter)&amp;&amp; cooldown&lt;=0{active=3;cooldown = 5;}
    if keyboard_check_pressed(vk_backspace)&amp;&amp; cooldown&lt;=0{active=1;cooldown = 5;}
}
if (active==3){
    //volume
    if keyboard_check(vk_left){global.maxvol-=.01}
    if keyboard_check(vk_right){global.maxvol+=.01}
    global.maxvol = clamp(global.maxvol,0,1)
    if keyboard_check_pressed(vk_enter)&amp;&amp; cooldown&lt;=0{
        active = 0;
    }
    if keyboard_check_pressed(vk_backspace)&amp;&amp; cooldown&lt;=0{active=2;cooldown = 5;}
}
if active{
    if keyboard_check_pressed(vk_escape){
        cooldown = 5;
        active = 0;
    }
    
}
//cooldown
cooldown--;
</string>
          </argument>
        </arguments>
      </action>
