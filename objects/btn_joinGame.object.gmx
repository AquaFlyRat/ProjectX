        <arguments>
          <argument>
            <kind>1</kind>
            <string>///record
if (active==1){
    var k = "";
    k = digit_check();
    if nat ==1 &amp;&amp; string_length(n1)&lt;nlength {n1+=k}
    if nat ==2 &amp;&amp; string_length(n2)&lt;nlength {n2+=k}
    if nat ==3 &amp;&amp; string_length(n3)&lt;nlength {n3+=k}
    if nat &gt;=4 &amp;&amp; string_length(n4)&lt;nlength {n4+=k}
    //n
    if string_length(n1)&gt;=nlength &amp;&amp; nat ==1{nat++}
    if string_length(n2)&gt;=nlength &amp;&amp; nat ==2{nat++}
    if string_length(n3)&gt;=nlength &amp;&amp; nat ==3{nat++}
    //back
    if keyboard_check_pressed(vk_backspace){
        var i = 1;
        if i &amp;&amp; nat &gt;=4 &amp;&amp; string_length(n4)&gt;0{n4 = string_copy(n4,0,string_length(n4)-1);i--;}
        if i &amp;&amp; nat &gt;=4 &amp;&amp; string_length(n4)&lt;=0{nat=3;}
        if i &amp;&amp; nat ==3 &amp;&amp; string_length(n3)&gt;0{n3 = string_copy(n3,0,string_length(n3)-1);i--;}
        if i &amp;&amp; nat ==3 &amp;&amp; string_length(n3)&lt;=0{nat=2;}
        if i &amp;&amp; nat ==2 &amp;&amp; string_length(n2)&gt;0{n2 = string_copy(n2,0,string_length(n2)-1);i--;}
        if i &amp;&amp; nat ==2 &amp;&amp; string_length(n2)&lt;=0{nat=1;}
        if i &amp;&amp; nat ==1 &amp;&amp; string_length(n1)&gt;0{n1 = string_copy(n1,0,string_length(n1)-1);i--;}
    }
    ///total
    var fill = "_-------------"
    var s = 0;
    if nat !=1{s = 4}else{s=0}
    var fn1 = string_copy(fill,s,3-string_length(n1));
    if nat !=2{s = 4}else{s=0}
    var fn2 = string_copy(fill,s,3-string_length(n2));
    if nat !=3{s = 4}else{s=0}
    var fn3 = string_copy(fill,s,3-string_length(n3));
    if nat &lt;4{s = 4}else{s=0}
    var fn4 = string_copy(fill,s,3-string_length(n4));
    ipname = string(n1)+string(fn1)+"."+string(n2)+string(fn2)+"."+string(n3)+string(fn3)+"."+string(n4)+string(fn4);
    ///advance
    if keyboard_check_released(vk_space) || keyboard_check_released(vk_tab){
        if nat&lt;4{nat++;}
    }
    if keyboard_check_released(vk_enter) &amp;&amp; cooldown&lt;=0{
        active=2;
        cooldown = 5;
    }
}
if (active==2){
    var k = "";
    k = digit_check();
    portname+=k;
    //back
    if keyboard_check_pressed(vk_backspace){
        if string_length(portname)&lt;=0{
            active--;
        }
        portname = string_copy(portname,0,string_length(portname)-1)
    }
    if string_length(portname)&gt;=portname_length{
        portname = string_copy(portname,0,portname_length)
    }
    if keyboard_check_released(vk_enter) &amp;&amp; cooldown&lt;=0{
        active=3;
        cooldown = 5;
    }
}
if (active==3){
    var k = "";
    k = letter_check();
    nickname+=k;
    //back
    if keyboard_check_pressed(vk_backspace){
        if string_length(nickname)&lt;=0{
            active--;
        }
        nickname = string_copy(nickname,0,string_length(nickname)-1)
    }
    if string_length(nickname)&gt;=nn_length{
        nickname = string_copy(nickname,0,nn_length)
    }
    if keyboard_check_released(vk_enter) &amp;&amp; cooldown&lt;=0{
        ///add nickname to client
    }
}
//cooldown
cooldown--;
</string>
          </argument>
