            <kind>1</kind>
            <string>///draw
//background
var vw = view_wview*obj_view.view_zoom;
var vh = view_hview*obj_view.view_zoom;
var sc = 1;
draw_set_color(c_black);
draw_set_alpha(guifade);
draw_rectangle(-8,-8,(vw)+8,(vh)+8,false);
if (gamestart){
    //draw rules etc
    if (guifade &lt; 1) {guifade = lerp(guifade,1,.1)}
    var p1 = "You are Innocent";
    var p2 = "find and kill the traitor"
    text_draw(vw*.5-text_length(p1,spr_font_a,sc)*.5,vh*.5-7,p1,spr_font_a,c_white,sc,1*guifade,false);
    text_draw(vw*.5-text_length(p2,spr_font_a,sc)*.5,vh*.5+8,p2,spr_font_a,c_white,sc,1*guifade,false);
}
if (gameon) {
    if (guifade &gt; 0) {guifade = lerp(guifade,0,.1)}
    //draw timer
    var t = time;
    var ms = t;
    var sec = 0;
    var mn = 0;
    while ms&gt;=room_speed{
        ms-=room_speed;
        sec++;
        while sec&gt;=60{
            sec-=60;
            mn++;
        }
    }
    if sec&lt;10{
        sec = "0"+string(sec);
    }
    var p1 = "time:"+string(mn)+":"+string(sec);    //+"."+string(ms);
    text_draw(vw-48-text_length(p1,spr_font_a,sc),vh-40,p1,spr_font_a,c_white,sc,1-(1*guifade),false);
}
if (gameover) {
    if (guifade &lt; 1) {guifade = lerp(guifade,1,.1)}
    if (i_win) {
        //innocent win
        var p1 = "the innocent win!";
        var p2 = "the traitor is inferior!"
        text_draw(vw*.5-text_length(p1,spr_font_a,sc)*.5,vh*.5-7,p1,spr_font_a,c_white,sc,1*guifade,false);
        text_draw(vw*.5-text_length(p2,spr_font_a,sc)*.5,vh*.5+8,p2,spr_font_a,c_white,sc,1*guifade,false);
    }
    if (t_win) {
        //traitor win
        var p1 = "the Traitor wins!";
        var p2 = "you idiots..."
        text_draw(vw*.5-text_length(p1,spr_font_a,sc)*.5,vh*.5-7,p1,spr_font_a,c_white,sc,1*guifade,false);
        text_draw(vw*.5-text_length(p2,spr_font_a,sc)*.5,vh*.5+8,p2,spr_font_a,c_white,sc,1*guifade,false);
    }
    if (!t_win &amp;&amp; !i_win) &amp;&amp; time&gt;0{
        //draw: dead
        var p1 = "everyone died";
        var p2 = "you are all unworthy"
        text_draw(vw*.5-text_length(p1,spr_font_a,sc)*.5,vh*.5-7,p1,spr_font_a,c_white,sc,1*guifade,false);
        text_draw(vw*.5-text_length(p2,spr_font_a,sc)*.5,vh*.5+8,p2,spr_font_a,c_white,sc,1*guifade,false);
    }
    if (!t_win &amp;&amp; !i_win) &amp;&amp; time&lt;=0{
        //draw: time ran out
        var p1 = "time has expired";
        var p2 = "What a bunch of quick thinkers!"
        text_draw(vw*.5-text_length(p1,spr_font_a,sc)*.5,vh*.5-7,p1,spr_font_a,c_white,sc,1*guifade,false);
        text_draw(vw*.5-text_length(p2,spr_font_a,sc)*.5,vh*.5+8,p2,spr_font_a,c_white,sc,1*guifade,false);
    }
}
guifade = clamp(guifade,0,1);
draw_set_alpha(1);
</string>
